    "dirty": "^1.1.0",
    "docopt": "^0.6.2",
    "github": "^9.2.0",
    "mkdirp": "^0.5.1",
    "openpgp": "^2.5.4",
    "pseudomap": "^1.0.2",
    "request": "^2.81.0",
    "rollbar": "^2.0.4",
    "username": "^2.3.0"
  },
  "devDependencies": {
    "pkg": "^4.0.0"
